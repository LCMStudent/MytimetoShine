# Solarized App - Environment Variables Example
# Copy this file to .env and fill in your actual API keys

# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Solar API Key (usually same as Google Maps key)
VITE_SOLAR_API_KEY=your_solar_api_key_here

# Default map configuration (adjust for your region)
VITE_DEFAULT_LAT=49.9929
VITE_DEFAULT_LNG=8.2473
VITE_DEFAULT_ZOOM=13

# Development settings
VITE_USE_MOCK_SOLAR_DATA=false
